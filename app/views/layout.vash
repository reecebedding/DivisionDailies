<!doctype html>
<html>
    <head>
        <title>
        @if(model.title != null){
            @model.title
        }else{
           var title = model.__('layout.title')
           @title
        }
        </title>
    </head>
    <body>
        <section id='body'>
            @html.block('body')
        </section>
        
        @html.block('scripts')
    </body>
</html>